<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_color"
    tools:context=".ui.home.HomeFragment"
    >






    <TextView
        android:id="@+id/text_home"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@id/text_home"
        app:layout_constraintHorizontal_bias="0.514"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/text_home"
        />



    <View
        android:id="@+id/dataWrapper"
        android:layout_width="385dp"
        android:layout_height="40dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="584dp"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/solid_button"
        app:layout_constraintTop_toTopOf="@+id/time" />


    <TextView
        android:id="@+id/SwitchLoggingTitle"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_marginStart="224dp"
        android:layout_marginTop="216dp"
        android:text="Data \nLogging"
        app:layout_constraintStart_toStartOf="@id/GaimWrapper"
        app:layout_constraintTop_toTopOf="@+id/GaimWrapper" />

    <TextView
        android:id="@+id/SwitchStartTitle"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="216dp"
        android:text="Data Acquisition"
        app:layout_constraintStart_toStartOf="@id/GaimWrapper"
        app:layout_constraintTop_toTopOf="@+id/GaimWrapper" />

    <Switch
        android:id="@+id/DataStartSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="123dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="@id/GaimWrapper"
        app:layout_constraintTop_toBottomOf="@+id/GaimWrapper" />

    <Switch
        android:id="@+id/DataLoggingSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="284dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="@id/GaimWrapper"
        app:layout_constraintTop_toBottomOf="@+id/GaimWrapper" />

    <View
        android:id="@+id/dataWrapper2"
        android:layout_width="385dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="70dp"
        android:background="@drawable/solid_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/time" />

    <View
        android:id="@+id/dataWrapper3"
        android:layout_width="385dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/solid_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/time" />

    <View
        android:id="@+id/dataKind"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="340dp"
        android:background="@drawable/datakind_solid"
        android:scrollbarAlwaysDrawHorizontalTrack="false"
        android:scrollbarAlwaysDrawVerticalTrack="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/dataWrapper"
        app:layout_constraintTop_toTopOf="@id/dataWrapper" />

    <View
        android:id="@+id/dataKind2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/datakind_solid"
        android:scrollbarAlwaysDrawHorizontalTrack="false"
        android:scrollbarAlwaysDrawVerticalTrack="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/dataWrapper2"
        app:layout_constraintTop_toTopOf="@id/dataWrapper2" />



    <View
        android:id="@+id/dataKind3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/datakind_solid"
        android:scrollbarAlwaysDrawHorizontalTrack="false"
        android:scrollbarAlwaysDrawVerticalTrack="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/dataWrapper3"
        app:layout_constraintTop_toTopOf="@id/dataWrapper3" />

    <View
        android:id="@+id/PPHQTitle"
        android:layout_width="90dp"
        android:layout_height="150dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="15dp"
        android:translationZ="1dp"
        android:background="@drawable/datakind_solid"
        android:scrollbarAlwaysDrawHorizontalTrack="false"
        android:scrollbarAlwaysDrawVerticalTrack="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/PPHQWrapper"
        app:layout_constraintTop_toTopOf="@id/PPHQWrapper" />

    <View
        android:id="@+id/GaimTitle"
        android:layout_width="90dp"
        android:layout_height="150dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="15dp"
        android:translationZ="1dp"
        android:background="@drawable/datakind_solid"
        android:scrollbarAlwaysDrawHorizontalTrack="false"
        android:scrollbarAlwaysDrawVerticalTrack="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/GaimWrapper"
        app:layout_constraintTop_toTopOf="@id/GaimWrapper" />

    <TextView
        android:id="@+id/UTCTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginTop="9dp"
        android:fontFamily="@font/opensansregular"
        android:text="UTC"
        android:textColor="#000000"
        android:textSize="9dp"
        app:layout_constraintStart_toStartOf="@id/dataKind"
        app:layout_constraintTop_toTopOf="@+id/dataKind" />

    <TextView
        android:id="@+id/GSTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginTop="9dp"
        android:fontFamily="@font/opensansregular"
        android:text="GS"
        android:textColor="#000000"
        android:textSize="9dp"
        app:layout_constraintStart_toStartOf="@id/dataKind3"
        app:layout_constraintTop_toTopOf="@+id/dataKind3" />

    <TextView
        android:id="@+id/PPHQ"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="80dp"
        android:translationZ="2dp"
        android:fontFamily="@font/opensansregular"
        android:text="PPHQ"
        android:textColor="#000000"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@id/PPHQWrapper"
        app:layout_constraintTop_toTopOf="@+id/PPHQWrapper" />

    <TextView
        android:id="@+id/GaimResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="80dp"
        android:translationZ="2dp"
        android:fontFamily="@font/opensansregular"
        android:text=" Gaim"
        android:textColor="#000000"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@id/GaimWrapper"
        app:layout_constraintTop_toTopOf="@+id/GaimWrapper" />


    <TextView
        android:id="@+id/KSTTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginTop="9dp"

        android:fontFamily="@font/opensansregular"
        android:text="KST"
        android:textColor="#000000"
        android:textSize="9dp"
        app:layout_constraintStart_toStartOf="@id/dataKind2"
        app:layout_constraintTop_toTopOf="@+id/dataKind2" />

    <TextView
        android:id="@+id/UtcTimeList"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="11dp"
        android:fontFamily="@font/opensansbold"
        android:text= "Data Loading..."
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="@id/dataKind"
        app:layout_constraintTop_toTopOf="@id/dataWrapper" />

    <TextView
        android:id="@+id/GSTimeList"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="11dp"
        android:fontFamily="@font/opensansbold"
        android:text="Data Loading..."
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="@id/dataKind3"
        app:layout_constraintTop_toTopOf="@id/dataWrapper3" />

    <TextView
        android:id="@+id/PPHQList"
        android:layout_width="250dp"
        android:translationZ="2dp"
        android:layout_height="130dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="25dp"
        android:fontFamily="@font/opensansbold"
        android:text="Data Loading..."
        android:textColor="#000000"
        android:textSize="9sp"
        android:scrollbars="vertical"
        app:layout_constraintStart_toStartOf="@id/PPHQTitle"
        app:layout_constraintTop_toTopOf="@id/PPHQWrapper" />


    <TextView
        android:id="@+id/GaimResultList"
        android:layout_width="250dp"
        android:translationZ="2dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="25dp"
        android:layout_height="130dp"
        android:fontFamily="@font/opensansbold"
        android:text="Data Loading..."
        android:scrollbars="vertical"
        android:textColor="#000000"
        android:textSize="9sp"
        app:layout_constraintStart_toStartOf="@id/GaimTitle"
        app:layout_constraintTop_toTopOf="@id/GaimWrapper" />


    <TextView
        android:id="@+id/KSTtimeList"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="11dp"
        android:fontFamily="@font/opensansbold"
        android:text="Data Loading..."
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="@id/dataKind2"
        app:layout_constraintTop_toTopOf="@id/dataWrapper2" />


    <View
        android:id="@+id/PPHQWrapper"
        android:layout_width="385dp"
        android:layout_height="180dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="180dp"
        android:background="@drawable/solid_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/time" />

    <View
        android:id="@+id/GaimWrapper"
        android:layout_width="385dp"
        android:layout_height="180dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="370dp"
        android:background="@drawable/solid_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/time" />


</androidx.constraintlayout.widget.ConstraintLayout>